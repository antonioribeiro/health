{
  "name": "pragmarx/health",

  "description": "Laravel Server & App Health Monitor and Notifier",

  "keywords": ["health", "laravel", "pragmarx", "notifications", "panel", "monitor", "server", "app"],

  "license": "BSD-3-Clause",

  "authors": [
