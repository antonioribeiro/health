  },

  "suggest": {
    "guzzlehttp/guzzle": "~6.0",
    "docusign/esign-client": "^2.0",
    "predis/predis": "^1.0",
    "guzzlehttp/guzzle": "~6.0",
    "league/flysystem-aws-s3-v3": "^1.0"
  },

  "autoload": {
    "files": [
      "src/Support/Throwable.php",
      "src/Support/helpers.php"
